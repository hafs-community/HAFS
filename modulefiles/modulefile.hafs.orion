setenv WGRIB2 /apps/contrib/NCEP/libs/hpc-stack/intel-2018.4/impi-2018.4/wgrib2/2.0.8/bin/wgrib2

module load nco/4.9.3

module load rocoto/1.3.3

module load intelpython3/2020

# For CDEPS data models:
module load cdo
