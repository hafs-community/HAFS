#PBS -N %RUN%%STORMNUM%_wav_prep_%CYC%
#PBS -j oe
#PBS -S /bin/bash
#PBS -q %QUEUE%
#PBS -A %PROJ%-%PROJENVIR%
#PBS -l walltime=01:00:00
#PBS -l place=vscatter:excl,select=1:ncpus=40:mpiprocs=40:ompthreads=1
